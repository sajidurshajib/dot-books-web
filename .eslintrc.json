{
    "extends": ["react-app", "prettier", "eslint:recommended","plugin:@typescript-eslint/eslint-recommended","plugin:@typescript-eslint/recommended"],
    "plugins": ["prettier"],
    "parser": "@babel/eslint-parser",
    "rules": {
        "no-console": "warn",
        "prettier/prettier": ["warn", { "printWidth": 120 }],
        "react/react-in-jsx-scope": "off",
        "@typescript-eslint/no-unused-vars": ["off"]
    }
}